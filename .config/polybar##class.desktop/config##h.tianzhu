include-file = /home/menno/.config/polybar/base

[bar/base]
modules-right = mpris loadavg cpu temperature memory network pulseaudio date

[bar/left]
inherit = bar/base
monitor = "DVI-D-0"

[bar/right]
inherit = bar/base
monitor = "DP-0"

tray-position = right

[module/temperature]
thermal-zone = 2

[module/network]
interface = wlp2s0
